{
  "manifest_version": 3,
  "name": "AOnotita",
  "version": "2.1",
  "description": "Resalta, anota y guarda tus notas en AO3.",
  "permissions": ["storage"],
  "icons": {
    "16": "icono.png",
    "48": "icono.png",
    "128": "icono.png"
  },
  "content_scripts": [
    {
      "matches": ["*://archiveofourown.org/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
